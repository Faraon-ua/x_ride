<h1>Listing assets</h1>

<table>
  <tr>
    <th>Asset file name</th>
    <th>Asset content type</th>
    <th>Asset file size</th>
    <th>Asset updated at</th>
    <th>Title</th>
    <th>Description</th>
    <th>Url</th>
    <th>Part</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @assets.each do |asset| %>
  <tr>
    <td><%= asset.asset_file_name %></td>
    <td><%= asset.asset_content_type %></td>
    <td><%= asset.asset_file_size %></td>
    <td><%= asset.asset_updated_at %></td>
    <td><%= asset.title %></td>
    <td><%= asset.description %></td>
    <td><%= asset.url %></td>
    <td><%= asset.part_id %></td>
    <td><%= link_to 'Show', asset %></td>
    <td><%= link_to 'Edit', edit_asset_path(asset) %></td>
    <td><%= link_to 'Destroy', asset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Asset', new_asset_path %>
